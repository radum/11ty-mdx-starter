    "prism-react-renderer": "^1.2.0",
    "purgecss": "^4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "safe-json-stringify": "^1.2.0",
    "styled-components": "^5.2.3",
    "tailwindcss": "^2.1.2",
